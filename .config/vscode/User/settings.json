{
    "diffEditor.codeLens": true,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.cursorSurroundingLines": 5,
    "editor.folding": true,
    "editor.fontFamily": "Hack",
    "editor.fontSize": 16,
    "editor.fontWeight": 420,
    "editor.trimAutoWhitespace": false,
    "editor.glyphMargin": true,
    "editor.occurrencesHighlight": "off",
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestions": {
        "other": "inline"
    },
    "editor.renderLineHighlight": "none",
    "editor.renderWhitespace": "all",
    "editor.snippetSuggestions": "inline",
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.preview": true,
    "workbench.editor.enablePreview": false,
    "editor.suggest.selectionMode": "never",
    "editor.suggest.showSnippets": false,
    "editor.suggest.showStatusBar": true,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestLineHeight": 36,
    "editor.suggestOnTriggerCharacters": false,
    "editor.suggestSelection": "recentlyUsed",
    "editor.wordWrap": "on",
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "extensions.ignoreRecommendations": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "files.autoSave": "afterDelay",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "git.openRepositoryInParentFolders": "never",
    "haskell.formattingProvider": "stylish-haskell",
    "haskell.manageHLS": "PATH",
    "haskell.plugin.fourmolu.config.external": true,
    "haskell.plugin.ghcide-type-lenses.config.mode": "diagnostics",
    "haskell.plugin.importLens.codeLensOn": false,
    "haskell.upgradeGHCup": false,
    "rust-analyzer.imports.granularity.enforce": true,
    "rust-analyzer.inlayHints.chainingHints.enable": false,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.renderColons": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "rust-analyzer.lens.enable": false,
    "rust-analyzer.typing.autoClosingAngleBrackets.enable": true,
    "terminal.integrated.fontFamily": "Jetbrains Mono, s0sevka m",
    "terminal.integrated.fontWeight": 360,
    "window.newWindowDimensions": "inherit",
    "workbench.layoutControl.enabled": false,
    "workbench.startupEditor": "none",
    "editor.guides.indentation": false,
    "workbench.list.smoothScrolling": true,
    "workbench.reduceMotion": "off",
    "workbench.colorTheme": "Owokai",
    "editor.cursorWidth": 3,
    "search.actionsPosition": "auto",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "terminal.external.linuxExec": "kitty",
    "editor.minimap.enabled": false,
    "update.mode": "manual",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 16,
    "errorLens.delay": 3000,
    "errorLens.onSave": true,
    "editor.smoothScrolling": true,
    "explorer.confirmDragAndDrop": false,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "rust-analyzer.check.ignore": [
        "unused_variables",
        "dead_code"
    ],
    "telemetry.telemetryLevel": "off",
    "window.zoomLevel": 1,
    "window.titleBarStyle": "custom",
    "launch": {
        "configurations": [
            {
                "type": "lldb-dap",
                "request": "attach",
                "name": "LLDB attach",
                "attachCommands": ["attach ${command:process-matcher.match}"]
            },
            {
                "type": "lldb-dap",
                "request": "attach",
                "name": "LLDB remote attach",
                "platformName": "remote-linux",
                "attachCommands": [
                    "platform select remote-linux",
                    "platform connect connect://${input:IP-port}",
                    "platform process attach --pid ${input:PID-on-host}",
                    "settings set target.source-map /home/gpadmin/gpdb_src ${workspaceFolder}"
            },
        ],
        "inputs": [
            {
                "id": "IP",
                "type": "promptString",
                "description": "Enter IP"
            },
            {
                "id": "PID",
                "type": "promptString",
                "description": "Enter a PID"
            },
        ],
    },
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.cursorStyle": "line-thin"
}
